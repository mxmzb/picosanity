{
  "name": "picosanity",
  "version": "2.0.2",
  "description": "Tiny Sanity client alternative should you only need to do queries",
  "main": "lib/index.js",
  "browser": "lib/browser.js",
  "umd": "umd/client.js",
  "unpkg": "umd/client.js",
  "scripts": {
    "clean": "rimraf lib/ umd/",
    "build": "npm run clean && npm run compile && NODE_ENV=production webpack -p",
    "compile": "babel --copy-files -d lib/ src/",
    "posttest": "eslint .",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rexxars/picosanity.git"
  },
  "keywords": [
    "sanity",
    "client"
  ],
  "engines": {
    "node": ">=8"
  },
  "author": "Espen Hovlandsdal <espen@hovlandsdal.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rexxars/picosanity/issues"
  },
  "homepage": "https://github.com/rexxars/picosanity#readme",
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.9.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-sanity": "^1.149.0",
    "jest": "^25.3.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "webpack": "^4.42.1",
    "webpack-command": "^0.5.0"
  }
}
